<script type="text/javascript">
    var day_labels = [];
    var week_labels = [];
    var month_labels = [];
    var labels = function(){
        for(var i=0;i<31;i++){
            day_labels[i] = i+1;
        }
        for(var i=0;i<52;i++){
            week_labels[i] = i+1;
        }
        for(var i=0;i<12;i++){
            month_labels[i] = i+1;
        }
    }

    labels();
</script>

